{
  "$schema": "https://smapi.io/schemas/content-patcher.json",
  "Changes": [
    {
      "Action": "Load",
      "Target": "Maps/{{ModId}}_MO_Crib",
      "FromFile": "assets/MO_Crib.tmx"
    },
    {
      "Action": "Load",
      "Target": "Maps/{{ModId}}_MO_Chimkins",
      "FromFile": "assets/MO_Chimkins.tmx"
    },
    {
      "Action": "Load",
      "Target": "Maps/{{ModId}}/SewerChickens",
      "FromFile": "assets/SewerChickens.png"
    },
    {
      "Action": "EditData",
      "Target": "mushymato.MMAP/MapOverrides",
      "Entries": {
        "{{ModId}}_Crib": {
          "Id": "{{ModId}}_Crib",
          "SourceMap": "Maps/{{ModId}}_MO_Crib",
          "SourceRect": {
            "X": 0,
            "Y": 0,
            "Width": 2,
            "Height": 4
          },
          "TargetRect": {
            "X": 10,
            "Y": 10,
            "Width": 3,
            "Height": 4
          }
        },
        "{{ModId}}_Chimkins": {
          "Id": "{{ModId}}_Chimkins",
          "SourceMap": "Maps/{{ModId}}_MO_Chimkins",
          "TargetRect": {
            "X": 10,
            "Y": 9,
            "Width": 1,
            "Height": 2
          },
          "Precedence": 1
        },
        "{{ModId}}_Chimkins_Relative": {
          "Id": "{{ModId}}_Chimkins_Relative",
          "SourceMap": "Maps/{{ModId}}_MO_Chimkins",
          "TargetRectIsRelative": true,
          "TargetRect": {
            "X": 3,
            "Y": -1,
            "Width": 1,
            "Height": 2
          },
        }
      }
    }
  ]
}
