{
  "$schema": "https://smapi.io/schemas/content-patcher.json",
  "Changes": [
    {
      "Action": "Load",
      "Target": "{{ModId}}/full,{{ModId}}/full2,{{ModId}}/horizontal,{{ModId}}/horizontal2,{{ModId}}/lamp_simple,{{ModId}}/simple,{{ModId}}/simple2,{{ModId}}/vertical,{{ModId}}/window_horizontal,{{ModId}}/2x1_horizontal,{{ModId}}/1x2_vertical,{{ModId}}/2x2_full,{{ModId}}/pot_simple,{{ModId}}/rug_simple,{{ModId}}/rug_full",
      "FromFile": "connected-textures/assets/{{TargetWithoutPath}}.png"
    },
    {
      "Action": "Include",
      "FromFile": "connected-textures/data/bigobjects.json, connected-textures/data/furniture.json"
    }
  ]
}