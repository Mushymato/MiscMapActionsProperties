{
  "$schema": "https://smapi.io/schemas/content-patcher.json",
  "Changes": [
    {
      "Action": "Load",
      "Target": "{{ModId}}/fishtank_horizontal,{{ModId}}/fishtankthin_horizontal",
      "FromFile": "fish-tank/assets/{{TargetWithoutPath}}.png"
    },
    {
      "Action": "EditData",
      "Target": "Data/Furniture",
      "Entries": {
        "{{ModId}}_fishtank_horizontal": "{{ModId}}_fishtank_horizontal/fishtank/2 4/2 1/1/1000/-1/{{ModId}}_fishtank_horizontal/0/{{ModId}}\\fishtank_horizontal/false",
        "{{ModId}}_fishtank_horizontal_wall": "{{ModId}}_fishtank_horizontal_wall/painting/2 4/2 3/1/1000/-1/{{ModId}}_fishtank_horizontal_wall/0/{{ModId}}\\fishtank_horizontal/false",
        "{{ModId}}_fishtankthin_horizontal_wall": "{{ModId}}_fishtankthin_horizontal_wall/painting/1 3/1 3/1/1000/-1/{{ModId}}_fishtankthin_horizontal_wall/0/{{ModId}}\\fishtankthin_horizontal/false",
        // "{{ModId}}_fishtankthin_horizontal_dresser": "{{ModId}}_fishtankthin_horizontal_dresser/dresser/1 3/1 1/1/1000/-1/{{ModId}}_fishtankthin_horizontal_dresser/0/{{ModId}}\\fishtankthin_horizontal/false"
      }
    },
    {
      "Action": "EditData",
      "Target": "mushymato.MMAP/FurnitureProperties",
      "Entries": {
        // calcifer_fishtank_c4x4y-128w128h164
        "{{ModId}}_fishtank_horizontal": {
          "CustomFields": {
            "FishTank": "4 4 -128 128 164"
          }
        },
        // calcifer_fishtank_c4x0y0w128h164
        "{{ModId}}_fishtank_horizontal_wall": {
          "CustomFields": {
            "FishTank": "4 0 0 128 164"
          }
        },
        // calcifer_fishtank_c2x0y40w64h120
        "{{ModId}}_fishtankthin_horizontal_wall": {
          "CustomFields": {
            "FishTank": "2 0 40 64 120"
          }
        },
      }
    },
    {
      "Action": "EditData",
      "Target": "mushymato.MMAP/ConnectedTextures",
      "Entries": {
        "(F){{ModId}}_fishtank_horizontal": {
          "Style": "Horizontal",
          "ConnectWith": [
            "(F){{ModId}}_fishtank_horizontal",
          ]
        },
        "(F){{ModId}}_fishtank_horizontal_wall": {
          "Style": "Horizontal",
          "ConnectWith": [
            "(F){{ModId}}_fishtank_horizontal_wall"
          ]
        },
        "(F){{ModId}}_fishtankthin_horizontal_wall": {
          "Style": "Horizontal",
          "ConnectWith": [
            "(F){{ModId}}_fishtankthin_horizontal_wall"
          ]
        }
      }
    }
  ]
}